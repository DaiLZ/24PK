<?xml version="1.0" encoding="UTF-8"?>
<web-app id="WebApp_ID"  version="4.0" xmlns="http://xmlns.jcp.org/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" >
  <display-name>For24</display-name>
  
  <servlet>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>priv.for24.servlet.LoginServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>RegisterServlet</servlet-name>
    <servlet-class>priv.for24.servlet.RegisterServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>GetQServlet</servlet-name>
    <servlet-class>priv.for24.servlet.GetQServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>GetuserServlet</servlet-name>
    <servlet-class>priv.for24.servlet.GetuserServlet</servlet-class>
  </servlet>
  
  <servlet>
    <servlet-name>RankServlet</servlet-name>
    <servlet-class>priv.for24.servlet.RankServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>RankServlet</servlet-name>
    <url-pattern>/RankServlet</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/LoginServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>RegisterServlet</servlet-name>
    <url-pattern>/RegisterServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>GetQServlet</servlet-name>
    <url-pattern>/GetQServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>GetuserServlet</servlet-name>
    <url-pattern>/GetuserServlet</url-pattern>
  </servlet-mapping>
  
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  
  <listener>
    <listener-class>priv.for24.listener.MatchListener</listener-class>
  </listener>
  
  <listener>
    <listener-class>priv.for24.listener.AnswerListener</listener-class>
  </listener>
 
</web-app>